{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": ["error", "windows"],
        "require-jsdoc" : 0,
        "max-len": ["error", { "code": 256 }],
        "react/prop-types": 0
    },
    "settings": {
        "react": {
        "version": "detect"
        }
    }
}
