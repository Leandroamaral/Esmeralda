{
  "expo": {
    "name": "esmeralda",
    "slug": "esmeralda",
    "scheme": "com.esmeralda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "translucent": true,
      "backgroundColor": "#FFF"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconEsmeralda.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCCR63hMWEDwv7q4uBKeSGuzcOGBLKG_2k"
        }
      },
      "package": "com.leobsb.esmeralda"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "cleideson",
    "extra": {
      "eas": {
        "projectId": "ba801d84-b755-4a82-b3b2-5e9d70298a7b"
      }
    }
  }
}
